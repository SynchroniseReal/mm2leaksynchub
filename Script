Main:addButton("Void Everyone",function()
	local args = {
		[1] = "SprayPaint"
	}

	game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))

	local players = game:GetService("Players")
	local localPlayer = players.LocalPlayer
	local backpack = localPlayer:WaitForChild("Backpack")
	local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()

	local function equipSprayPaint()
		local sprayPaintTool = backpack:FindFirstChild("SprayPaint")
		if sprayPaintTool then
			sprayPaintTool.Parent = character
		end
	end

	equipSprayPaint()

	local sprayPaintTool = character:WaitForChild("SprayPaint")
	local remote = sprayPaintTool:WaitForChild("Remote")

	for _, target in ipairs(players:GetPlayers()) do
		if target ~= localPlayer and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, target) then
			local args = {
				[1] = 0,
				[2] = Enum.NormalId.Front,
				[3] = 2048,
				[4] = target.Character.Head,
				[5] = target.Character.Head.CFrame * CFrame.new(1000000000, 1000000000, 1000000000)
			}
			remote:FireServer(unpack(args))
		end
	end
	game.StarterGui:SetCore("SendNotification",{
		Title = "Executed Successfully";
		Text = "🟢";
	})
	game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
end)

Main:addButton("Minecraft Blocks",function()
	if game.Players.LocalPlayer.Character ~= nil then
		local mouse = game.Players.LocalPlayer:GetMouse()
		tool = Instance.new("Tool")
		tool.RequiresHandle = false
		tool.Name = "Tree Planter"
		tool.Activated:connect(function()
			if game:GetService("Players").LocalPlayer.Backpack.Toys:FindFirstChild("SprayPaint") then
				game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
