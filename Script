Main:addToggle("Loop Kill Everyone",function(value)
	print(value)
	if value == false then 
		game.StarterGui:SetCore("SendNotification",{
			Title = "Toggle Status";
			Text = "🔴";
		})
	else 
		game.StarterGui:SetCore("SendNotification",{
			Title = "Toggle Status";
			Text = "🟢";
		})
	end
	_G.autokill = (value)

	while true do
		local players = game:GetService("Players")
		local localPlayer = players.LocalPlayer
		local character = localPlayer.Character
		local backpack = localPlayer.Backpack

		if _G.autokill then
			local args = {
				[1] = "SprayPaint"
			}

			game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
			local sprayPaintTool = character:FindFirstChild("SprayPaint") or backpack:FindFirstChild("SprayPaint")
			if sprayPaintTool and not character:FindFirstChild("SprayPaint") then
				sprayPaintTool.Parent = character
			end

			if character:FindFirstChild("SprayPaint") then
				local remote = character.SprayPaint.Remote

				for _, target in ipairs(players:GetPlayers()) do
					if target ~= localPlayer and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, target) then
						local args = {
							[1] = 80373024,
							[2] = Enum.NormalId.Front,
							[3] = 100000000,
							[4] = target.Character.HumanoidRootPart,
							[5] = CFrame.new(math.huge, math.huge, math.huge) * CFrame.Angles(-2.326180934906006, -1.285452127456665, -2.3468010425567627)
						}
						remote:FireServer(unpack(args))
					end
				end
			end
		end
		wait(1)
	end
end)

Main:addButton("Kill Murderer",function()
	if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("SprayPaint") then
		local args = {
			[1] = "SprayPaint"
		}

		game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
		game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Character
		for i, murd in pairs(game.Players:GetChildren()) do
			if murd.Backpack:FindFirstChild("Knife") or murd.Character:FindFirstChild("Knife") then
				local args = {
					[1] = 80373024,
					[2] = Enum.NormalId.Front,
					[3] = 100000000,
					[4] = murd.Character.HumanoidRootPart,
					[5] = CFrame.new(math.huge, math.huge, math.huge) * CFrame.Angles(-2.326180934906006, -1.285452127456665, -2.3468010425567627)
				}

				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
				game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
			end
		end
	elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("SprayPaint") then
		local args = {
			[1] = "SprayPaint"
		}

		game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
		for i, murd in pairs(game.Players:GetChildren()) do
			if murd.Backpack:FindFirstChild("Knife") or murd.Character:FindFirstChild("Knife") then
				local args = {
					[1] = 80373024,
					[2] = Enum.NormalId.Front,
					[3] = 100000000,
					[4] = murd.Character.HumanoidRootPart,
					[5] = CFrame.new(math.huge, math.huge, math.huge) * CFrame.Angles(-2.326180934906006, -1.285452127456665, -2.3468010425567627)
				}

				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
				game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
			end
		end
	else
		local args = {
			[1] = "SprayPaint"
		}

		game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
		game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Character
		for i, murd in pairs(game.Players:GetChildren()) do
			if murd.Backpack:FindFirstChild("Knife") or murd.Character:FindFirstChild("Knife") then
				local args = {
					[1] = 80373024,
					[2] = Enum.NormalId.Front,
					[3] = 100000000,
					[4] = murd.Character.HumanoidRootPart,
					[5] = CFrame.new(math.huge, math.huge, math.huge) * CFrame.Angles(-2.326180934906006, -1.285452127456665, -2.3468010425567627)
				}

				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
				game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
			end
		end
	end
	game.StarterGui:SetCore("SendNotification",{
		Title = "Murderer Successfully Killed";
		Text = "🟢";
	})
end)

