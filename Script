Main:addButton("Cage Everyone",function()
	local ID = 9425588896
	for i, v in pairs(game.Players:GetPlayers()) do
		if v ~= game.Players.LocalPlayer and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, v) then
			if game:GetService("Players").LocalPlayer.Backpack.Toys:FindFirstChild("SprayPaint") then
				local posofpl = workspace[v.Name].HumanoidRootPart.CFrame
				game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
				game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
				game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(ID, Enum.NormalId.Top, 6, workspace[v.Name].HumanoidRootPart, posofpl * CFrame.new(0, 3, 0))
				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(ID, Enum.NormalId.Bottom, 6, workspace[v.Name].HumanoidRootPart, posofpl * CFrame.new(0, 3.15, 0))
				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(ID, Enum.NormalId.Bottom, 6, workspace[v.Name].HumanoidRootPart, posofpl * CFrame.new(0, -2.8, 0))
				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(ID, Enum.NormalId.Top, 6, workspace[v.Name].HumanoidRootPart, posofpl * CFrame.new(0, -3, 0))
				game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(ID, Enum.NormalId.Front, 6, workspace[v.Name].HumanoidRootPart, posofpl * CFrame.new(0, 0.1, 3.1))
