	if string.len(value) >= 1 then
		for i,v in ipairs(game.Players:GetPlayers()) do
			local lowplayername = string.lower(v.Name)
			if string.find(lowplayername,value) then
				local Target = v

				function Penis()
					local args = {
						[1] = "SprayPaint"
					}

					game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
					game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Character
					-- Top Penis
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Top, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1,-0.7))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Top, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1,-1))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Top, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1,-1.5))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Top, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1,-2))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(13850207336, Enum.NormalId.Top, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1,-2.5))

					----Bottom Penis
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Bottom, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1.3,-0.5))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Bottom, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1.3,-1))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Bottom, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1.3,-1.5))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Bottom, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1.3,-2))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(13850207336, Enum.NormalId.Bottom, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(0,-1.3,-2.5))

					-- Left Penis
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Left, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.15,-1.15,-0.5))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Left, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.15,-1.15,-1))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Left, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.15,-1.15,-1.5))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Left, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.15,-1.15,-2))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(13850207336, Enum.NormalId.Left, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.15,-1.15,-2.5))

					-- Right Penis
