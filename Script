					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Front, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.5,-1.15,-0.85))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Bottom, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.5,-1.3,-0.7))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(12976059241, Enum.NormalId.Left, 0.5, (Target.Character.HumanoidRootPart), Target.Character.HumanoidRootPart.CFrame * CFrame.new(-0.65,-1.15,-0.7))
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
				end
			end
		end
	end

	Penis()
end)

