Main:addTextBox("Void Player","Player Name Here",function(value)
	if string.len(value) >= 1 then
		for i,v in ipairs(game.Players:GetPlayers()) do
			local lowplayername = string.lower(v.Name)
			if string.find(lowplayername,value) then
				local target = v
				if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("SprayPaint") then
					local args = {
						[1] = "SprayPaint"
					}

					game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
					game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Character
					local args = {
						[1] = 0,
						[2] = Enum.NormalId.Front,
						[3] = 2048,
						[4] = target.Character.Head,
						[5] = target.Character.Head.CFrame * CFrame.new(1000000000, 1000000000, 1000000000)
					}

					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
					game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
				elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("SprayPaint") then
					local args = {
						[1] = "SprayPaint"
					}

					game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
					local args = {
						[1] = 0,
						[2] = Enum.NormalId.Front,
						[3] = 2048,
						[4] = target.Character.Head,
						[5] = target.Character.Head.CFrame * CFrame.new(1000000000, 1000000000, 1000000000)
					}

					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
					game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
				else
					local args = {
						[1] = "SprayPaint"
					}

					game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
					game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Character
					local args = {
						[1] = 0,
						[2] = Enum.NormalId.Front,
						[3] = 2048,
						[4] = target.Character.Head,
						[5] = target.Character.Head.CFrame * CFrame.new(1000000000, 1000000000, 1000000000)
					}

					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
					game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
				end
			end
		end
	end
end)

