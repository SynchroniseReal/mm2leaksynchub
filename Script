LP:addButton("Reset Local Player",function()
	game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)

LP:addToggle("Circle Spin", function(value)
	local targetPlayerName = _G.circlename
	_G.L360spin = (value)
	local localPlayer = game.Players.LocalPlayer

	local function teleportAndSpin(targetPlayer)
		if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
			local targetRootPart = targetPlayer.Character.HumanoidRootPart
			local localCharacter = localPlayer.Character or localPlayer.CharacterAdded:Wait()
			local localRootPart = localCharacter:FindFirstChild("HumanoidRootPart")

			if localRootPart and _G.L360spin then
				local direction = (localRootPart.Position - targetRootPart.Position).Unit
				local newPosition = targetRootPart.Position + (direction * _G.Idksynchub)
				localRootPart.CFrame = CFrame.new(newPosition, targetRootPart.Position)

				spawn(function()
					while _G.L360spin do
						for i = 0, 360, _G.sexyspinspeed69 do
							if not _G.L360spin then break end
							local angle = math.rad(i)
							local offset = Vector3.new(math.cos(angle) * _G.Idksynchub, 0, math.sin(angle) * _G.Idksynchub)
							local newCFrame = CFrame.new(targetRootPart.Position + offset, targetRootPart.Position)
							localRootPart.CFrame = newCFrame
							task.wait()
						end
					end
				end)
			end
		end
	end

	local function onTargetCharacterAdded(character)
		local targetPlayer = game.Players:FindFirstChild(targetPlayerName)
		if targetPlayer then
			teleportAndSpin(targetPlayer)
		end
	end

	local function onLocalCharacterAdded(character)
		local targetPlayer = game.Players:FindFirstChild(targetPlayerName)
		if targetPlayer and targetPlayer.Character then
			teleportAndSpin(targetPlayer)
		else
			game.Players.PlayerAdded:Connect(function(player)
				if player.Name == targetPlayerName then
					player.CharacterAdded:Connect(function(character)
						teleportAndSpin(player)
					end)
				end
			end)
		end
	end

	localPlayer.CharacterAdded:Connect(onLocalCharacterAdded)

	if localPlayer.Character then
		onLocalCharacterAdded(localPlayer.Character)
	end

	local targetPlayer = game.Players:FindFirstChild(targetPlayerName)
	if targetPlayer and targetPlayer.Character then
		teleportAndSpin(targetPlayer)
	else
		game.Players.PlayerAdded:Connect(function(player)
			if player.Name == targetPlayerName then
				player.CharacterAdded:Connect(onTargetCharacterAdded)
			end
		end)
	end
end)

local Players = game:GetService("Players")
local ZestFest = {}

for _, player in pairs(Players:GetPlayers()) do
	table.insert(ZestFest, player.DisplayName)
end

LP:addDropdown("Circle Name selection", ZestFest, 4, function(value)
