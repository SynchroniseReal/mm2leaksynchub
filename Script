test:addToggle("Silent Aim",function(value)
	_G.silentaim = (value)

	local function notifyToggleStatus(toggleName, status)
		local NotificationBindable = Instance.new("BindableFunction")
		NotificationBindable.OnInvoke = function(Text)
			callback(Text)
		end

		game.StarterGui:SetCore("SendNotification", {
			Title = toggleName,
			Text = "Toggle is " .. status,
			Duration = 2,
			Callback = NotificationBindable,
		})
	end

	local function toggleSilentAim()
		_G.silentaim = not _G.silentaim
		notifyToggleStatus("Silent Aim", _G.silentaim and "ON" or "OFF")

		if not _G.silentaim then
			clearDrawings()
		end
	end

	local function toggleRainbowFov()
		_G.rainbowFov = not _G.rainbowFov
		notifyToggleStatus("Rainbow FOV Circle", _G.rainbowFov and "ON" or "OFF")
	end

	notifyToggleStatus("Silent Aim", _G.silentaim and "ON" or "OFF")
	notifyToggleStatus("Rainbow FOV Circle", _G.rainbowFov and "ON" or "OFF")

	local function callback(Text)
	end

	local NotificationBindable = Instance.new("BindableFunction")
	NotificationBindable.OnInvoke = callback

	local settings = {
		enabled = true,
		circleTransparency = 1,
		radius = _G.fovcirclesize,
		sides = _G.fovcirclesides,
		rainbow = _G.rainbowFov,
		color = Color3.fromRGB(0, 0, 0),
		offset = Vector2.new(0, 60),
		outline = _G.outlineFov,
		dynamic = true,
	}

	local runService = game:GetService('RunService')
	local camera = game.Workspace.CurrentCamera
	local tau = 2 * math.pi
	local drawings = {}

	local function createDrawings()
		for i = 1, settings.sides do
			drawings[i] = { Drawing.new('Line'), Drawing.new('Line') }
			drawings[i][1].ZIndex = 1
			drawings[i][1].Thickness = _G.fovcirclethickness
			drawings[i][2].ZIndex = 2
			drawings[i][2].Thickness = _G.coolbeans
		end
	end

	local function clearDrawings()
		for _, pair in ipairs(drawings) do
			pair[1]:Remove()
			pair[2]:Remove()
		end
		drawings = {}
	end

	createDrawings()

	local mouse = game.Players.LocalPlayer:GetMouse()
	local isRightMouseDown = false

	local function findPlayersInCircle(centerX, centerY, radius)
		local playersInCircle = {}

		for _, player in ipairs(game.Players:GetPlayers()) do
			if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild('HumanoidRootPart') then
				local torsoPosition = player.Character.HumanoidRootPart.Position
				local torsoScreenPosition, isOnScreen = camera:WorldToViewportPoint(torsoPosition)

				if isOnScreen then
					local distance = (Vector2.new(torsoScreenPosition.x, torsoScreenPosition.y) - Vector2.new(centerX, centerY)).magnitude
					if distance <= radius then
						table.insert(playersInCircle, { player = player, distance = distance })
					end
				end
			end
		end

		return playersInCircle
	end

	local function findClosestPlayer(centerX, centerY, radius)
		local playersInCircle = findPlayersInCircle(centerX, centerY, radius)
		local closestPlayer = nil
		local closestDistance = math.huge

		for _, data in ipairs(playersInCircle) do
			if data.distance < closestDistance then
				closestDistance = data.distance
				closestPlayer = data.player
			end
		end

		return closestPlayer
	end
	local function shootSilentAimBullet(player)
		if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			local Players = game:GetService("Players")
			local LocalPlayer = Players.LocalPlayer

			if LocalPlayer.Character:FindFirstChild("Gun") or LocalPlayer.Backpack:FindFirstChild("Gun") then
				local torsoPosition = player.Character.HumanoidRootPart.Position
				local torsoVelocity = player.Character.HumanoidRootPart.Velocity

				local predictedPosition = torsoPosition + torsoVelocity * 0.145
				local args = {
					[1] = 1,
					[2] = predictedPosition,
					[3] = "AH2"
				}

				game:GetService("Players").LocalPlayer.Character.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(args))

				local currentTime = tick()
				if currentTime - lastShotTime >= 1 then
					lastShotTime = currentTime
					local NotificationBindable = Instance.new("BindableFunction")
					NotificationBindable.OnInvoke = function(Text)
						callback(Text)
					end

					game.StarterGui:SetCore("SendNotification", {
						Title = "Locked Onto",
						Text = player.Name,
						Duration = 1,
						Callback = NotificationBindable,
					})
				end
			elseif LocalPlayer.Character:FindFirstChild("Knife") then
				local function findClosestPlayerToMouse()
					local Mouse = LocalPlayer:GetMouse()
					local closestDist = math.huge
					local closestPlayer = nil
					local mousePosition = Mouse.Hit.Position

					for _, player in pairs(Players:GetPlayers()) do
						if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
							local dist = (player.Character.HumanoidRootPart.Position - mousePosition).magnitude
							if dist < closestDist then
								closestDist = dist
								closestPlayer = player
							end
						end
					end

					return closestPlayer
				end

				local closestPlayer = findClosestPlayerToMouse()
				if closestPlayer then
					wait (0.5)
					local args = {
						[1] = LocalPlayer.Character.HumanoidRootPart.CFrame,
						[2] = closestPlayer.Character.HumanoidRootPart.Position
					}

					LocalPlayer.Character.Knife.Throw:FireServer(unpack(args))
				else
					warn("No players found nearby.")
				end
			else
				warn("No suitable weapon found.")
			end
		end
	end

	mouse.Button2Down:Connect(function()
		isRightMouseDown = true

		if _G.silentaim then
			local mouseX, mouseY = mouse.X, mouse.Y
			local closestPlayer = findClosestPlayer(mouseX, mouseY, settings.radius)

			if closestPlayer then
				shootSilentAimBullet(closestPlayer)
			end
		end
	end)

	mouse.Button2Up:Connect(function()
		isRightMouseDown = false
	end)

	runService.RenderStepped:Connect(function()
		if settings.enabled then
			local centerX, centerY = mouse.X, mouse.Y
			local radius = settings.radius

			if settings.dynamic then
				local fovScalingFactor = 1 / math.tan(math.rad(camera.FieldOfView / 2))
				radius = fovScalingFactor * settings.radius
			end

			local closestPlayer = findClosestPlayer(centerX, centerY, radius)

			if _G.silentaim then
				for i = 1, #drawings do
					local backLine = drawings[i][1]
					local line = drawings[i][2]

					local color = _G.rainbowFov and Color3.fromHSV((tick() % 5 / 5 - (i / #drawings)) % 1, 0.5, 1) or settings.color
					local pos = Vector2.new(centerX, centerY) + settings.offset
					local last = (i / settings.sides) * tau
					local next = ((i + 1) / settings.sides) * tau
					local lastX = pos.X + math.cos(last) * radius
					local lastY = pos.Y + math.sin(last) * radius
					local nextX = pos.X + math.cos(next) * radius
					local nextY = pos.Y + math.sin(next) * radius

					if settings.outline then
						backLine.From = Vector2.new(lastX, lastY)
						backLine.To = Vector2.new(nextX, nextY)
						backLine.Color = Color3.new(0, 0, 0)
						backLine.Transparency = settings.circleTransparency
						backLine.Visible = true
					else
						backLine.Visible = false
					end

					line.From = Vector2.new(lastX, lastY)
					line.To = Vector2.new(nextX, nextY)
					line.Color = color
					line.Transparency = settings.circleTransparency
					line.Visible = true
				end

				if isRightMouseDown and closestPlayer then
					shootSilentAimBullet(closestPlayer)
				end
			else
				clearDrawings()
			end
		end
	end)


end)

test:addToggle("Rainbow Circle Fov",function(value)
	_G.rainbowFov = value
end)

test:addToggle("Fov Circle Outline",function(value)
	_G.outlineFov = value
end)

test:addSlider("Fov Circle Sides", 4, 25, function(value)
	_G.fovcirclesides = value
end)

test:addSlider("Fov Circle Size", 5, 500, function(value)
	_G.fovcirclesize = value
end)

test:addSlider("Fov Circle Outline Thickness", 2, 10, function(value)
	_G.fovcirclethickness = value
end)

test:addSlider("Fov Circle Thickness", 2, 10, function(value)
	_G.coolbeans = value
end)
