Main2:addButton("Fake Die",function()
	local Human = game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass('Humanoid')
	if not Human then
		return
	end
	Human.Sit = true
	task.wait(.1)
	Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
	for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
		v:Stop()
	end
end)

Main2:addToggle("Auto Kill All When Murder",function(autokillall)
	autokillallloop = autokillall
	while autokillallloop do
		function autokillallloopfix()
			EquipTool()
			wait()
			local localCharacter = game.Players.LocalPlayer.Character
			local knife = localCharacter and localCharacter:FindFirstChild("Knife")
			wait()
			for _, player in ipairs(game.Players:GetPlayers()) do
				if player ~= game.Players.LocalPlayer then
					wait()
					local playerCharacter = player.Character
					local humanoidRootPart = playerCharacter and playerCharacter:FindFirstChild("HumanoidRootPart")

					if humanoidRootPart then
						Stab()
						firetouchinterest(humanoidRootPart, knife.Handle, 1)
						firetouchinterest(humanoidRootPart, knife.Handle, 0)
					end
				end
			end
			wait()
		end
		wait()
		pcall(autokillallloopfix)
	end
end)

Main2:addButton("Open Dev Console",function()
	game.StarterGui:SetCore("DevConsoleVisible", true)
end)

Main2:addToggle("Xray",function(xray)
	local t=false

	local function scan(z,t)
		for _,i in pairs(z:GetChildren()) do
			if i:IsA("BasePart") and not i.Parent:FindFirstChild("Humanoid") and not i.Parent.Parent:FindFirstChild("Humanoid") then
				i.LocalTransparencyModifier=t
			end
			scan(i,t)
		end
	end
	function x(v)
		if v then
			if xray == true then
				scan(workspace,0.9)
			else
				scan(workspace,0)
			end
		else
			scan(workspace,0)
		end
	end
	t=not t
	x(t)
end)

Main2:addToggle("Spam Trade Request",function(value)
	if value == false then
		_G.reqtrade = false
	else
		_G.reqtrade = true
		while wait(0.01) do
			if not _G.reqtrade then break end
			local args = {
				[1] = _G.Tradespamtarget
			}

			game:GetService("ReplicatedStorage").Trade.SendRequest:InvokeServer(unpack(args))
		end
end
end)

Main2:addTextBox("Spam Trade Player","Player Name Here",function(value)
	if string.len(value) >= 1 then
		for i,v in ipairs(game.Players:GetPlayers()) do
			local lowplayername = string.lower(v.Name)
			if string.find(lowplayername,value) then
				local Target = v
				if Target then
					_G.Tradespamtarget = Target
					game.StarterGui:SetCore("SendNotification", {
						Title = "Selected player is";
						Text = "" .. Target.Name .. "";
					})
				else
					game.StarterGui:SetCore("SendNotification", {
						Title = "Player Not Found";
						Text = "The player '" .. Target.Name .. "' does not exist.";
					})
				end
			end
		end
	end
end)
Main:addLabel("Spray paint features","⬇️")

Main:addButton("Kill Everyone",function()
	local args = {
		[1] = "SprayPaint"
	}

	game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))

	local players = game:GetService("Players")
	local localPlayer = players.LocalPlayer
	local backpack = localPlayer:WaitForChild("Backpack")
	local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()

	local function equipSprayPaint()
		local sprayPaintTool = backpack:FindFirstChild("SprayPaint")
		if sprayPaintTool then
			sprayPaintTool.Parent = character
		end
	end

	equipSprayPaint()

	local sprayPaintTool = character:WaitForChild("SprayPaint")
	local remote = sprayPaintTool:WaitForChild("Remote")
	wait(1)
	for _, target in ipairs(players:GetPlayers()) do
		if target ~= localPlayer and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, target) then
			local args = {
				[1] = 80373024,
				[2] = Enum.NormalId.Front,
				[3] = 100000000,
				[4] = target.Character.HumanoidRootPart,
				[5] = CFrame.new(math.huge, math.huge, math.huge) * CFrame.Angles(-2.326180934906006, -1.285452127456665, -2.3468010425567627)
			}
			remote:FireServer(unpack(args))
		end
	end
	game.StarterGui:SetCore("SendNotification",{
		Title = "Executed Successfully";
		Text = "🟢";
	})
end)

Main:addButton("Fling Everyone",function()
	if game.Players.LocalPlayer.Character ~= nil then
		for i, v in pairs(game.Players:GetPlayers()) do
			if v ~= game.Players.LocalPlayer and v.Character ~= nil and v.Character:FindFirstChild("LeftHand") and not table.find(_G.excludedplayers, v) then
				local args = {
					[1] = 0,
					[2] = Enum.NormalId.Back,
					[3] = 15,
					[4] = workspace[v.Name].LeftHand,
					[5] = workspace[v.Name].LeftHand.CFrame * CFrame.new(0, _G.Flingstrength, 0)
				}
				if game:GetService("Players").LocalPlayer.Backpack.Toys:FindFirstChild("SprayPaint") then
					game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
					game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
					game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
					game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
				elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("SprayPaint") then
					game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
					game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
				elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("SprayPaint") then
					game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
				end
			end
		end
	end
	game.StarterGui:SetCore("SendNotification",{
		Title = "Executed Successfully";
		Text = "🟢";
	})
end)

Main:addSlider("Fling Strength",1,100,function(value)
	_G.Flingstrength = value
end)

Main:addButton("Noclip Everyone",function()
	local players = game:GetService("Players")
	local localPlayer = players.LocalPlayer
	local replicatedStorage = game:GetService("ReplicatedStorage")
	replicatedStorage.Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
	localPlayer.Backpack.SprayPaint.Parent = localPlayer.Character
	for _, v in pairs(players:GetPlayers()) do
		if v ~= localPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, v) then
			local sprayArgs = {
				0,
				Enum.NormalId.Top,
				1500,
				v.Character:FindFirstChild("HumanoidRootPart"),
				v.Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 10000, 0)
			}

			localPlayer.Character.SprayPaint.Remote:FireServer(unpack(sprayArgs))
		end
	end
	localPlayer.Character.SprayPaint.Parent = localPlayer.Backpack
	game.StarterGui:SetCore("SendNotification",{
		Title = "ALL NOCLIP STATUS:";
		Text = "TRUE";
	})
	for i = 15, 1, -1 do
		local countdownText = tostring(i)
		if i > 1 then
			countdownText = countdownText .. "s"
		end
		game.StarterGui:SetCore("SendNotification", {
			Title = "ALL NOCLIP TIME";
			Text = countdownText;
			Duration = 0.5;
		})
		wait(1)
	end

	game.StarterGui:SetCore("SendNotification",{
		Title = "ALL NOCLIP STATUS:";
		Text = "FALSE";
	})
end)

Main:addButton("Temporary God Mode (can't pick up anything)",function()
	local args = {
		[1] = 0,
		[2] = Enum.NormalId.Top,
		[3] = 6.331,
		[4] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"),
		[5] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0,10010,0)
	}

	if game:GetService("Players").LocalPlayer.Backpack.Toys:FindFirstChild("SprayPaint") then
		game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
		game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
		game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
		game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
		game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
	elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("SprayPaint") then
		game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
		game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
		game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
	elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("SprayPaint") then
		game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
	end
	game.StarterGui:SetCore("SendNotification",{
		Title = "GOD MODE STATUS:";
		Text = "TRUE";
	})
	for i = 15, 1, -1 do
		local countdownText = tostring(i)
		if i > 1 then
			countdownText = countdownText .. "s"
		end
		game.StarterGui:SetCore("SendNotification", {
			Title = "LOCAL GODODE TIME";
			Text = countdownText;
			Duration = 0.5;
		})
		wait(1)
	end

	game.StarterGui:SetCore("SendNotification",{
		Title = "LOCAL GODMODE STATUS:";
		Text = "FALSE";
	})

end)

Main:addTextBox("Noclip Player", "Player Name Here", function(value)
	if string.len(value) >= 1 then
		for i,v in ipairs(game.Players:GetPlayers()) do
			local lowplayername = string.lower(v.Name)
			if string.find(lowplayername,value) then
				local target = v
				local args = {
					[1] = "SprayPaint"
				}

				game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
				game.Players.LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
				if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
					local sprayArgs = {
						[1] = 0,
						[2] = Enum.NormalId.Top,
						[3] = 1500,
						[4] = target.Character:FindFirstChild("HumanoidRootPart"),
						[5] = target.Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 10000, 0)
					}
					game.Players.LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(sprayArgs))
				end
				game.Players.LocalPlayer.Character.SprayPaint.Parent = game.Players.LocalPlayer.Backpack
			end
		end
	end
end)

Main:addToggle("Loop Kill Player",function(value)
	if value == false then
		_G.loopkillplayer:Disconnect()
	else
		_G.loopkillplayer = game:GetService("RunService").RenderStepped:Connect(function()
			local p = _G.loopkillperson
			if p and p.Character ~= nil and p.Character:FindFirstChild("HumanoidRootPart") then
				if game.Players.LocalPlayer.Character ~= nil then
					local args = {
						[1] = 80373024,
						[2] = Enum.NormalId.Back,
						[3] = 15,
						[4] = p.Character.HumanoidRootPart,
						[5] = CFrame.new(0, math.huge, 0)
					}
					if game:GetService("Players").LocalPlayer.Backpack.Toys:FindFirstChild("SprayPaint") then
						game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
						game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
						game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
						game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
						game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
					elseif game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("SprayPaint") then
						game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game.Players.LocalPlayer.Character
						game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
						game.Players.LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
					elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("SprayPaint") then
						game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(unpack(args))
					end
				end
			end
		end)
	end
end)

