			local backpack = player:WaitForChild("Backpack")
			local knife = backpack:FindFirstChild("Knife") or character:FindFirstChild("Knife")
			if knife then
				if knife.Parent == backpack then
					knife.Parent = character
				end
				for _, v in ipairs(Players:GetPlayers()) do
					if v ~= player and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, v) then
						local enemyRoot = v.Character.HumanoidRootPart
						firetouchinterest(enemyRoot, knife.Handle, 1)
						wait(0.03)
						firetouchinterest(enemyRoot, knife.Handle, 0)
					end
				end
			end
		elseif fullballsbag and game.Players.LocalPlayer.Backpack:FindFirstChild("Gun") then
			game.Players.LocalPlayer.Backpack:FindFirstChild("Gun").Parent = game.Players.LocalPlayer.Character
			for i, murd in pairs(game.Players:GetChildren()) do
				if murd.Backpack:FindFirstChild("Knife") or murd.Character:FindFirstChild("Knife") then
					_G.test = true
					local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					spawn(function()
						while wait(0.001) do
							if not _G.test then break end
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = murd.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,3)
						end
					end)
					wait(0.2)
					spawn(function()
						local args = {
							[1] = 1,
							[2] = murd.Character.HumanoidRootPart.Position,
							[3] = "AH2"
						}

						game:GetService("Players").LocalPlayer.Character.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(args))
					end)
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
					_G.test = false
				end
			end
		end
	end
end)

Main2:addToggle("The best auto farm by sigma male",function(value)
	_G.autofarm = (value)
	local safezone = Instance.new("Part")
	safezone.Anchored = true
	safezone.Position = Vector3.new(0,10000,0)
	safezone.Size = Vector3.new(10,1,10)
	safezone.Parent = game.Workspace

	local collectedCoins = {}

	while _G.autofarm do
		pcall(function()
			local humanoid = game.Players.LocalPlayer.Character.Humanoid
			if humanoid ~= nil and humanoid.Health > 0 then
				local normalFolder = game.Workspace:FindFirstChild("Normal")
				if normalFolder then
					local coinContainer = normalFolder:WaitForChild("CoinContainer")
					if coinContainer then
						local coins = coinContainer:GetChildren()
						for i, coin in ipairs(coins) do
							if not collectedCoins[coin] then
								local position = coin.Position + Vector3.new(0, 5, 0)

								humanoid.RootPart.CFrame = CFrame.new(position)
								wait(0.75)

								collectedCoins[coin] = true
								break
							end
						end

						humanoid.RootPart.CFrame = safezone.CFrame + Vector3.new(0, 3, 0)
						wait(2)
					else
						wait(3)
					end
				else
					wait(3)
				end
			else
				while humanoid.Health <= 0 do
					wait()
				end
			end
		end)

		if not _G.autofarm then
			break
		end

		wait(0.1)
	end
end)

Main2:addToggle("Only Beach ball autofarm",function(value)
	_G.autofarm = (value)
	local safezone = Instance.new("Part")
	safezone.Anchored = true
	safezone.Position = Vector3.new(0,10000,0)
	safezone.Size = Vector3.new(10,1,10)
	safezone.Parent = game.Workspace

	local collectedball = {}

	while _G.autofarm do
		pcall(function()
			local humanoid = game.Players.LocalPlayer.Character.Humanoid
			if humanoid ~= nil and humanoid.Health > 0 then
				local normalFolder = game.Workspace:FindFirstChild("Normal")
				if normalFolder then
					local coinContainer = normalFolder:WaitForChild("CoinContainer")
					if coinContainer then
						local coins = coinContainer:GetChildren()
						for i, ball in ipairs(coins) do
							if not collectedball[ball] and not ball.CoinVisual:FindFirstChild("MainCoin") then
								local position = ball.Position + Vector3.new(0, 5, 0)

								humanoid.RootPart.CFrame = CFrame.new(position)
								wait(0.75)

								collectedball[ball] = true
								break
							end
						end

						humanoid.RootPart.CFrame = safezone.CFrame + Vector3.new(0, 3, 0)
						wait(2)
					else
						wait(3)
					end
				else
					wait(3)
				end
			else
				while humanoid.Health <= 0 do
					wait()
				end
			end
		end)

		if not _G.autofarm then
			break
		end

		wait(0.1)
	end
end)

Main2:addButton("Shoot Murderer",function()
	if game.Players.LocalPlayer.Character:FindFirstChild("Gun") then
		for i, murd in pairs(game.Players:GetChildren()) do
			if murd.Backpack:FindFirstChild("Knife") or murd.Character:FindFirstChild("Knife") then
				_G.test = true
				local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				spawn(function()
					while wait(0.001) do
						if not _G.test then break end
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = murd.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,3)
					end
				end)
				wait(0.2)
				spawn(function()
					local args = {
						[1] = 1,
						[2] = murd.Character.HumanoidRootPart.Position,
						[3] = "AH2"
					}

					game:GetService("Players").LocalPlayer.Character.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(args))
				end)
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
				_G.test = false
			end
		end
	else
		game.StarterGui:SetCore("SendNotification",{
			Title = "Please equip the gun";
			Text = "";
		})
	end
end)

Main2:addButton("Get Every Emote",function()
	local Remote = game.ReplicatedStorage.Remotes.Extras.GetPlayerData:InvokeServer("GetData")
	local Client = game.Players.LocalPlayer
	local ReplicatedStorage = game:GetService('ReplicatedStorage')
	local Modules = ReplicatedStorage.Modules
	local EmoteModule = Modules.EmoteModule
	local Emotes = Client.PlayerGui.MainGUI.Game:FindFirstChild("Emotes")
	local EmoteList = {"headless","zombie","zen","ninja","floss","dab","sit"}
	require(EmoteModule).GeneratePage(EmoteList,Emotes,'Your Emotes')
end)

Main2:addButton("Grab Gun",function()
	if game.Workspace.Normal:FindFirstChild("GunDrop") then
		local player = game.Players.LocalPlayer
		local character = player.Character
		local humanoidRootPart = character.HumanoidRootPart
		local gundrop = game.Workspace.Normal:FindFirstChild("GunDrop")
		local gunDropCFrame = gundrop.CFrame
		local initialCFrame = humanoidRootPart.CFrame
		humanoidRootPart.CFrame = gunDropCFrame * CFrame.new(0,0.5,0)
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(1)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 1)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 0)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 1)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 0)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 1)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 0)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 1)
		task.wait()
		firetouchinterest(humanoidRootPart, gundrop, 0)
		repeat wait() until game.Players.LocalPlayer.Backpack:FindFirstChild("Gun")
		humanoidRootPart.CFrame = initialCFrame
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(1)
	else
		game.StarterGui:SetCore("SendNotification",{
			Title = "Gun is not dropped";
			Text = "";
		})
	end
end)

Main2:addButton("Second Life",function()
	game.Players.LocalPlayer.Character.Humanoid.Name = 1
	local l = game.Players.LocalPlayer.Character["1"]:Clone()
	l.Parent = game.Players.LocalPlayer.Character
	l.Name = "Humanoid"
	wait(0.1)
	game.Players.LocalPlayer.Character["1"]:Destroy()
	game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
	game.Players.LocalPlayer.Character.Animate.Disabled = true
	wait(0.1)
	game.Players.LocalPlayer.Character.Animate.Disabled = false
	game.Players.LocalPlayer.Character.Humanoid.DisplayDistanceType = "None"

end)

Main2:addToggle("Anti Trap",function(antitrap)
	antitraploop = antitrap
	while antitraploop do
		function antitraploopfix()
			if LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed == 0.009999999776482582 then
				LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = 16
			end
			wait()
		end
		wait()
		pcall(antitraploopfix)
	end
end)

Main2:addToggle("Auto Accept Trades",function(autoaccept)
	autoacceptloop = autoaccept
	while autoacceptloop do
		game:GetService("ReplicatedStorage").Trade.AcceptTrade:FireServer()
		wait()
	end
end)

Main2:addToggle("Hide Guis",function(hidetradegui)
	hidetradeguiloop = true
	while hidetradeguiloop do
		function hidetradeguiloopfix()
			local gui = game:service'Players'.LocalPlayer:WaitForChild'PlayerGui':FindFirstChild("TradeGUI")
			if not gui then return end
			gui.Enabled = false
			local frame = gui:FindFirstChild("Container")
			if not frame then return end
			frame.Visible = false
			wait()
		end
		wait()
		pcall(hidetradeguiloopfix)
	end
end)

Main2:addButton("Destroy All Guns | Client Sided",function()
	for i,v in pairs(workspace:getDescendants()) do
		if v:IsA('Tool') then
			v:Destroy()
		end
	end
	task.wait()
end)

Main2:addToggle("Loop Fire Interactives",function(loopinteractive)
	loopinteractiveloop = loopinteractive
	while loopinteractiveloop do
		function loopinteractiveloopfix()
			if workspace:FindFirstChild("Bank2") then
				workspace.Bank2.Interactive.VaultSystem.InteractiveBox.Interact:FireServer()
			else
				if workspace:FindFirstChild("Factory") then
					workspace.Factory.LorryDoor.InteractiveBox.Interact:FireServer()
				else
					if workspace:FindFirstChild("MilBase") then
						workspace.MilBase.Door.InteractiveBox.Interact:FireServer()
					else
						if workspace:FindFirstChild("ResearchFacility") then
							workspace.ResearchFacility.Interactive.ScanSystem.Scanner.InteractiveBox.Interact:FireServer()
							workspace.ResearchFacility.Interactive.SirenSystem.InteractiveBox.Interact:FireServer()
							workspace.ResearchFacility.Interactive.CloningSystem.InteractiveBox.Interact:FireServer()
							workspace.ResearchFacility.Interactive.GarageSystem.InteractiveBox.Interact:FireServer()
						end
					end
				end
			end
			wait()
		end
		wait()
		pcall(loopinteractiveloopfix)
	end
end)

