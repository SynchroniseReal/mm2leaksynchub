Main:addButton("Zombify Yourself",function()
	local Target = game:GetService("Players").LocalPlayer
	local args = {
		[1] = "SprayPaint"
	}

	game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer(unpack(args))
	game:GetService("Players").LocalPlayer.Backpack.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Character
	game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(7520185837, Enum.NormalId.Top, 3.5, Target.Character.HumanoidRootPart, Target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 4.3, 0.5))
	game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(7520185837, Enum.NormalId.Bottom, 3.5, Target.Character.HumanoidRootPart, Target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1, 0.5))
	game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(7520185837, Enum.NormalId.Back, 3.5, Target.Character.HumanoidRootPart, Target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 2.65, 2.15))
	game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(13555045049, Enum.NormalId.Front, 3.5, Target.Character.HumanoidRootPart, Target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 2.65, -1.15))
	game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(7520185837, Enum.NormalId.Right, 3.5, Target.Character.HumanoidRootPart, Target.Character.HumanoidRootPart.CFrame * CFrame.new(1.65, 2.65, 0.5))
	game:GetService("Players").LocalPlayer.Character.SprayPaint.Remote:FireServer(7520185837, Enum.NormalId.Left, 3.5, Target.Character.HumanoidRootPart, Target.Character.HumanoidRootPart.CFrame * CFrame.new(-1.65, 2.65, 0.5))

	game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")
	game:GetService("ReplicatedStorage").Remotes.Extras.ReplicateToy:InvokeServer("SprayPaint")

	game:GetService("Players").LocalPlayer.Character.SprayPaint.Parent = game:GetService("Players").LocalPlayer.Backpack
end)

