Main2:addToggle("FE Trail",function(value)
	if value == false then
		game:GetService("Workspace")[game.Players.LocalPlayer.Name].SpeedTrail.Toggle:FireServer(false)
	else
		game:GetService("Workspace")[game.Players.LocalPlayer.Name].SpeedTrail.Toggle:FireServer(true)
	end
end)

Main2:addButton("Break Gun",function()
	for i, gun in pairs(game.Players:GetChildren()) do
		if gun.Backpack:FindFirstChild("Gun") then
			local args = {
				[1] = 1,
				[2] = CFrame.new(29382038, 238023, 293829),
				[3] = "AH2"
			}

			gun.Backpack.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(args))
		elseif gun.Character:FindFirstChild("Gun") then
			local args = {
				[1] = 1,
				[2] = CFrame.new(29382038, 238023, 293829),
				[3] = "AH2"
			}

			gun.Character.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(args))
		end
	end

end)

Main2:addToggle("Auto Farm",function(value)
	if value == false then
		_G.autofarm = false
	else
		_G.autofarm = true
		local Players = game:GetService("Players")
		local TweenService = game:GetService("TweenService")
		local RunService = game:GetService("RunService")

		local LOCAL_PLAYER = Players.LocalPlayer
		local CHARACTER = LOCAL_PLAYER.Character or LOCAL_PLAYER.CharacterAdded:Wait()
		local HUMANOID_ROOT_PART = CHARACTER:WaitForChild("HumanoidRootPart")
		local COIN_CONTAINER = game.workspace.Normal:WaitForChild("CoinContainer")

		local TWEEN_INFO = TweenInfo.new(
			5,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.In,
			0,
			false,
			0
		)

		local function moveTo(cframe)
			if not _G.autofarm then return end
			local goal = {}
			goal.CFrame = cframe
			local tween = TweenService:Create(HUMANOID_ROOT_PART, TWEEN_INFO, goal)
			tween:Play()
			tween.Completed:Wait()
		end


		local function fireTouchInterest(part)
			if not _G.autofarm then return end
			firetouchinterest(HUMANOID_ROOT_PART, part, 0)
			RunService.Heartbeat:Wait()
			firetouchinterest(HUMANOID_ROOT_PART, part, 1)
		end

		local function collectCoins()
			if not _G.autofarm then return end
			for _, coin in pairs(COIN_CONTAINER:GetChildren()) do
				if coin:IsA("BasePart") then
					moveTo(coin.CFrame)
					fireTouchInterest(coin)
				end
			end
		end

		game.Players.LocalPlayer.CharacterAdded:Connect(function()
			if not _G.autofarm then return end
			while wait(1) do
				if not _G.autofarm then break end
				collectCoins()
			end
		end)

		while wait(1) do
			if not _G.autofarm then break end
			collectCoins()
		end
	end
end)

local function checkfullcoins()
	if game:GetService("Players").LocalPlayer.PlayerGui.MainGUI.Game.CoinBags.Container.Coin.FullBagIcon.Visible == true then
		fullcoinbag = true
		return fullcoinbag
	end
end

Main2:addToggle("Auto kill when full coins",function(value)
	_G.autokillcoins = (value)
	while wait(1) do
		if not _G.autokillcoins then break end
		checkfullcoins()
		if fullcoinbag and game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Knife") then
			game:GetService("Players").LocalPlayer.Backpack.Knife.Parent = game:GetService("Players").LocalPlayer.Character
			wait (0.1)
			local args = {
				[1] = "Slash"
			}

			game:GetService("Players").LocalPlayer.Character.Knife.Stab:FireServer(unpack(args))
			local Players = game:GetService("Players")
			local player = Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local backpack = player:WaitForChild("Backpack")
			local knife = backpack:FindFirstChild("Knife") or character:FindFirstChild("Knife")
			if knife then
				if knife.Parent == backpack then
					knife.Parent = character
				end
				for _, v in ipairs(Players:GetPlayers()) do
					if v ~= player and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and not table.find(_G.excludedplayers, v) then
						local enemyRoot = v.Character.HumanoidRootPart
						firetouchinterest(enemyRoot, knife.Handle, 1)
						wait(0.03)
						firetouchinterest(enemyRoot, knife.Handle, 0)
					end
				end
			end
		elseif fullballsbag and game.Players.LocalPlayer.Backpack:FindFirstChild("Gun") then
			game.Players.LocalPlayer.Backpack:FindFirstChild("Gun").Parent = game.Players.LocalPlayer.Character
			for i, murd in pairs(game.Players:GetChildren()) do
				if murd.Backpack:FindFirstChild("Knife") or murd.Character:FindFirstChild("Knife") then
					_G.test = true
					local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					spawn(function()
						while wait(0.001) do
							if not _G.test then break end
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = murd.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,3)
						end
					end)
					wait(0.2)
					spawn(function()
						local args = {
							[1] = 1,
							[2] = murd.Character.HumanoidRootPart.Position,
							[3] = "AH2"
						}

						game:GetService("Players").LocalPlayer.Character.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(args))
					end)
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
					_G.test = false
				end
			end	
		end
	end
end)

local function checkfullballs()
	if game:GetService("Players").LocalPlayer.PlayerGui.MainGUI.Game.CoinBags.Container.BeachBall.FullBagIcon.Visible == true then
		fullballsbag = true
		return fullballsbag
	end
end

Main2:addToggle("Auto kill when full balls",function(value)
	_G.autokillcoins = (value)
	while wait(1) do
		if not _G.autokillcoins then break end
		checkfullballs()
		if fullballsbag and game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Knife") then
			game:GetService("Players").LocalPlayer.Backpack.Knife.Parent = game:GetService("Players").LocalPlayer.Character
			wait (0.1)
			local args = {
				[1] = "Slash"
			}

			game:GetService("Players").LocalPlayer.Character.Knife.Stab:FireServer(unpack(args))
			local Players = game:GetService("Players")
			local player = Players.LocalPlayer
